  "description": "一个基于vue2和vue3的右键菜单插件",
  "main": "index.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "package": "vue-cli-service build --target lib --name RightMenu --dest ./dist/ ./src/index.js",
